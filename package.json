{
  "name": "typespeed",
  "version": "2.0.11",
  "description": "A new Framework for TypeScript.",
  "author": "speedphp",
  "license": "MIT License",
  "scripts": {
    "build": "tsc -p .",
    "postbuild": "cp -r src/default/pages dist/default/pages && cp -r src/scaffold/templates dist/scaffold/templates",
    "prebuild": "rm -rf dist",
    "prepublishOnly": "npm version patch && npm run build",
    "upgrade": "npm version minor",
    "postpublish": "git push && git push --tags"
  },
  "main": "dist/typespeed.js",
  "bin": {
    "typespeed": "./dist/scaffold/command.js"
  },
  "homepage": "https://github.com/speedphp/typespeed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speedphp/typespeed.git"
  },
  "dependencies": {
    "@types/node": "^18.0.6",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.7.4",
    "walk-sync": "^3.0.0",
    "tracer": "^1.1.6",
    "express": "^4.18.1",
    "consolidate": "^0.16.0",
    "mustache": "^4.2.0",
    "serve-favicon": "^2.5.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "multiparty": "^4.2.3",
    "express-basic-auth": "^1.2.1",
    "express-jwt": "^7.7.5",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "ioredis": "^5.2.3",
    "connect-redis": "^6.1.3",
    "cron": "^2.1.0",
    "commander": "^9.4.0"
  }
}
