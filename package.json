{
  "name": "typespeed",
  "version": "2.3.6",
  "description": "A new Framework for TypeScript.",
  "author": "speedphp",
  "license": "MIT License",
  "scripts": {
    "test": "mocha --require ts-node/register test/**/*.ts --exit",
    "test-with-coverage": "nyc --reporter=lcov npm test",
    "start": "ts-node --transpile-only app/src/main.ts",
    "build": "tsc -p .",
    "postbuild": "cp src/typespeed.d.ts dist/typespeed.d.ts && cp -r src/default/pages dist/default/pages && cp -r src/scaffold/templates dist/scaffold/templates",
    "prebuild": "rm -rf dist",
    "prepublishOnly": "npm version patch && npm run build",
    "upgrade": "npm version minor"
  },
  "main": "dist/typespeed.js",
  "bin": {
    "typespeed": "./dist/scaffold/command.js"
  },
  "homepage": "https://github.com/speedphp/typespeed#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/speedphp/typespeed.git"
  },
  "keywords": [
    "framework",
    "express",
    "web",
    "app",
    "api",
    "model",
    "middleware"
  ],
  "dependencies": {
    "commander": "^9.4.0",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "consolidate": "1.0.1",
    "cookie-parser": "^1.4.6",
    "cron": "^2.3.0",
    "express": "^4.18.1",
    "express-basic-auth": "^1.2.1",
    "express-jwt": "^7.7.5",
    "express-session": "^1.17.3",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "multiparty": "^4.2.3",
    "mustache": "^4.2.0",
    "mysql2": "^2.3.3",
    "node-cache": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "serve-favicon": "^2.5.0",
    "tracer": "^1.1.6",
    "walk-sync": "^3.0.0",
    "amqplib": "0.10.3"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "@types/express": "^4.17.13",
    "@types/mocha": "^10.0.1",
    "chai": "4.3.7",
    "chai-http": "^4.4.0",
    "mocha": "10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
